package Shopping;

public class Product {
	String name;
	double price;
	long quantity;

	public Product(String name,double price,long quantity) {
		this.name=name;
		this.price=price;
		this.quantity=quantity;
	}

	@Override
	public String toString() {
	return "product[name=" +name + ", price=" + price + ", quantity=" + quantity + "]";
	}

}

